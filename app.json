{
    "name": "Music Bot",
    "description": "A Modular telegram song bot.",
    "repository": "https://github.com/Rexinazor/Music-Downloader",
    "keywords": [
       
        "telegram bot",
        "pyrogram"

    ],
    "env": {
        "APP_ID": {
            "description": "Get this value from my.telegram.org",
            "value": ""
        },
        "API_HASH": {
            "description": "Get this value from my.telegram.org",
            "value": ""
        },
        "OWNER": {
            "description": "Your username without @.",
            "value": "ZenoByte"
        },
        "START_MSG": {
            "description": "Start Message of BoT",
            "value": "<b>Hey {},\nI am Music Downloader Bot,</b>\n\nSend me Any Songs name directly or with /song command"
        },
        "START_IMG": {
            "description": "Start Message's Photo.",
            "value": "https://telegra.ph/file/1579bfa1c8394f308df1e.png"
        },
        "BOT_TOKEN": {
            "description": "ID of the message to edit.",
            "value": ""
        }
    },
    "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
        }
    ]
